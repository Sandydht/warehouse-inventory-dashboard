{
  "name": "warehouse-inventory-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prettier:format": "npx prettier . --write",
    "prettier:check": "npx prettier . --check",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.11.2",
    "@tailwindcss/vite": "4.1.18",
    "axios": "1.13.2",
    "clsx": "2.1.1",
    "crypto-js": "4.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.71.1",
    "react-redux": "9.2.0",
    "react-router-dom": "6.30.3",
    "tailwindcss": "4.1.18",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/crypto-js": "4.2.2",
    "@types/node": "24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jsdom": "28.0.0",
    "lint-staged": "16.2.7",
    "msw": "2.12.7",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite": "7.2.4",
    "vitest": "4.0.18"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
